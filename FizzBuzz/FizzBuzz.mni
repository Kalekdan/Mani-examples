/* FizzBuzz in Mani
If a value is divisible by 3 display Fizz
If a value is divisible by 5 display Buzz
If divisible by both then display FizzBuzz
Otherwise display the value
*/

let max = 100;

for (let i = 1; i <= max; i++){
    let output = "";
    if (i % 3 == 0){
        output += "Fizz";
    }
    if (i % 5 == 0){
        output += "Buzz";
    }
    if (output == ""){
        output = i;
    }
    say output;
}