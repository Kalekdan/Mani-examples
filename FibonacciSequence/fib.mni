let n1 = 0;
let n2 = 1;
let n3;
let count = 10; 

say n1;
say n2;      //saying 0 and 1

for(let i = 2; i < count; i = i + 1) {    
    
    //loop starts from 2 because 0 and 1 are already printed        
    n3 = n1 + n2;
    say n3;
    n1 = n2;
    n2 = n3;    
}   